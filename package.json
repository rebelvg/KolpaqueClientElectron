{
  "name": "KolpaqueClientElectron",
  "version": "0.4.7",
  "description": "Multiplatform GUI for Streamlink",
  "main": "./dist-api/main.js",
  "scripts": {
    "prepush": "yarn run lint",
    "lint": "eslint . --ext .ts && prettier --list-different \"**/*.{tsx,ts,js,json,css,md}\"",
    "lint:fix": "eslint . --ext .ts --fix && prettier --write \"**/*.{tsx,ts,js,json,css,md}\"",
    "build:api": "tsc --project api/tsconfig.json",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.config.production",
    "build": "yarn run build:api && yarn run build:app && ts-node build.ts",
    "start:api": "yarn build:api && cross-env NODE_ENV=dev electron .",
    "start:app": "webpack-dev-server",
    "start": "yarn run build:api && yarn run build:app && electron ."
  },
  "author": "KLPQ",
  "license": "CC0-1.0",
  "private": true,
  "workspaces": [
    "api",
    "app"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "@types/node": "^8.0.0",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "archiver": "^4.0.1",
    "cross-env": "^5.0.5",
    "electron": "8.2.3",
    "electron-devtools-installer": "^2.2.3",
    "electron-packager": "^14.2.1",
    "eslint": "^7.13.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^1.3.1",
    "prettier": "^2.2.0",
    "readline-sync": "^1.4.7",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-default-menu": "^1.0.1",
    "fix-path": "^2.1.0",
    "lodash": "^4.17.4",
    "querystring": "^0.2.0",
    "socket.io-client": "^2.3.0",
    "uuid": "^8.0.0"
  }
}
