{
  "name": "KolpaqueClientElectron",
  "packageManager": "pnpm@10.29.1",
  "version": "0.6.25",
  "description": "Multiplatform GUI for Streamlink",
  "main": "./api/dist/main.js",
  "scripts": {
    "prepush": "pnpm run lint",
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "lint": "eslint . && pnpm run prettier && pnpm --filter api run typecheck",
    "lint:fix": "eslint . --fix && pnpm run prettier:fix",
    "pack": "ts-node build.ts",
    "release": "ts-node release.ts",
    "build:api": "pnpm --filter api run build",
    "build:app": "pnpm --filter app run build",
    "start:api": "pnpm --filter api run start",
    "start:app": "pnpm --filter app run start",
    "start:dev": "cross-env REACT_ENV=dev concurrently --kill-others \"pnpm run start:api\" \"pnpm run start:app\"",
    "start": "pnpm run build:api && pnpm run build:app && pnpm run start:api",
    "prepare": "husky"
  },
  "author": "KLPQ",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.19.10",
    "@types/readline-sync": "^1.4.8"
  },
  "dependencies": {
    "@electron/packager": "^19.0.1",
    "archiver": "^4.0.1",
    "electron": "^39.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^5.0.5",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "readline-sync": "^1.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
