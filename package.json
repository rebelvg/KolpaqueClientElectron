{
  "name": "KolpaqueClientElectron",
  "version": "0.6.25",
  "description": "Multiplatform GUI for Streamlink",
  "main": "./api/dist/main.js",
  "scripts": {
    "prepush": "yarn run lint",
    "typecheck": "tsc --project api/tsconfig.json --noEmit",
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "lint": "eslint . && yarn run prettier && yarn run typecheck",
    "lint:fix": "eslint . --fix && yarn run prettier:fix",
    "pack": "ts-node pack.ts",
    "release": "ts-node release.ts",
    "build:api": "tsc --project api/tsconfig.json",
    "build:app": "yarn workspace app run build",
    "start:api": "cross-env NODE_ENV=dev electron . --ozone-platform=wayland",
    "start:app": "yarn workspace app run start",
    "start:dev": "cross-env REACT_ENV=dev concurrently --kill-others \"yarn run build:api && yarn run start:api\" \"yarn run start:app\"",
    "start": "yarn run build:api && yarn run build:app && yarn run start:api",
    "prepare": "husky"
  },
  "author": "KLPQ",
  "license": "CC0-1.0",
  "private": true,
  "workspaces": [
    "api",
    "app"
  ],
  "devDependencies": {
    "@electron/packager": "^19.0.3",
    "@types/archiver": "^6.0.3",
    "@types/lodash": "^4.17.23",
    "@types/node": "^24.9.2",
    "@types/readline-sync": "^1.4.8",
    "@types/semver": "^7.5.8",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^11.0.0",
    "archiver": "^4.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.4.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "readline-sync": "^1.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "electron-default-menu": "1.0.1",
    "fix-path": "^2.1.0",
    "lodash": "^4.17.23",
    "querystring": "^0.2.0",
    "semver": "^7.7.4",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.19",
    "uuid": "^13.0.0",
    "youtubei.js": "^16.0.1"
  }
}
