{
  "name": "KolpaqueClient",
  "version": "0.2.6",
  "description": "Multiplatform GUI for Streamlink",
  "main": "./dist-api/main.js",
  "scripts": {
    "prepush": "yarn run lint",
    "lint": "prettier --list-different \"**/*.{ts,js,json,css,md}\"",
    "lint:fix": "prettier --write \"**/*.{ts,js,json,css,md}\"",
    "build:api": "tsc",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.config.production",
    "build": "yarn run build:api && yarn run build:app && ts-node build.ts",
    "start:api": "yarn build:api && cross-env NODE_ENV=dev electron .",
    "start:app": "webpack-dev-server",
    "start": "yarn run build:api && yarn run build:app && electron ."
  },
  "author": "KLPQ",
  "license": "CC0-1.0",
  "private": true,
  "workspaces": [
    "application",
    "src"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "cross-env": "^5.0.5",
    "electron": "^3.0.11",
    "electron-packager": "^11.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "readline-sync": "^1.4.7",
    "ts-node": "^8.4.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-cli": "^2.0.6",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
