{
  "name": "KolpaqueClientElectron",
  "version": "0.5.16",
  "description": "Multiplatform GUI for Streamlink",
  "main": "./dist-api/main.js",
  "scripts": {
    "prepush": "yarn run lint",
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "lint": "eslint . && yarn run prettier",
    "lint:fix": "eslint . --fix && yarn run prettier:fix",
    "build:api": "tsc --project api/tsconfig.json",
    "build:app": "cross-env NODE_ENV=production webpack --config webpack.config.production",
    "build": "yarn run build:api && yarn run build:app && ts-node build.ts",
    "release": "yarn ts-node release.ts",
    "start:api": "yarn build:api && cross-env NODE_ENV=dev electron .",
    "start:app": "webpack-dev-server",
    "start:dev": "concurrently --kill-others \"yarn run start:api\" \"yarn run start:app\"",
    "start": "yarn run build:api && yarn run build:app && electron ."
  },
  "author": "KLPQ",
  "license": "CC0-1.0",
  "private": true,
  "workspaces": [
    "api",
    "app"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "archiver": "^4.0.1",
    "concurrently": "^5.3.0",
    "cross-env": "^5.0.5",
    "electron": "^16.0.0",
    "electron-packager": "^14.2.1",
    "eslint": "^7.13.0",
    "husky": "^1.3.1",
    "prettier": "^2.2.0",
    "readline-sync": "^1.4.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "axios": "^0.24.0",
    "electron-default-menu": "^1.0.1",
    "fix-path": "^2.1.0",
    "lodash": "^4.17.4",
    "querystring": "^0.2.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.19",
    "uuid": "^8.0.0"
  }
}
